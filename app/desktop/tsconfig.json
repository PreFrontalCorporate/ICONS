{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "types": ["vite/client", "node"],
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["src/renderer/**/*"]
}
